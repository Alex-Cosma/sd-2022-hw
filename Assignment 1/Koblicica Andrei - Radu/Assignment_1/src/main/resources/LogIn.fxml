<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LogInController">
   <children>
      <Label alignment="CENTER" layoutX="278.0" layoutY="146.0" prefHeight="46.0" prefWidth="259.0" text="Bank Application">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" alignment="CENTER_RIGHT" layoutX="292.0" layoutY="212.0" prefHeight="37.0" prefWidth="233.0" promptText="username" />
      <TextField fx:id="passwordField" alignment="CENTER_RIGHT" layoutX="291.0" layoutY="270.0" prefHeight="38.0" prefWidth="234.0" promptText="password" />
      <Button fx:id="logInButton" layoutX="292.0" layoutY="328.0" mnemonicParsing="false" onAction="#pressLogIn" prefHeight="38.0" prefWidth="107.0" text="Log In">
         <font>
            <Font size="16.0" />
         </font></Button>
      <Button fx:id="signUpButton" layoutX="418.0" layoutY="328.0" mnemonicParsing="false" onAction="#pressSignUp" prefHeight="38.0" prefWidth="107.0" text="Sign Up">
         <font>
            <Font size="16.0" />
         </font></Button>
   </children>
</AnchorPane>
